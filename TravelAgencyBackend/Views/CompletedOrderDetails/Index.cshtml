@model IEnumerable<TravelAgency.Shared.Models.CompletedOrderDetail>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DocumentMenu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderForm)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentMenu.MenuId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderForm.ChineseName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CompletedOrderDetailId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CompletedOrderDetailId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CompletedOrderDetailId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
